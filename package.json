{
  "name": "hid-relay",
  "version": "1.0.3",
  "description": "Connect to and control USB relays through the HID interface",
  "keywords": [
    "usb",
    "hid",
    "relay"
  ],
  "homepage": "https://github.com/matt-oakes/node-hid-relay#readme",
  "bugs": {
    "url": "https://github.com/matt-oakes/node-hid-relay/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-oakes/node-hid-relay.git"
  },
  "license": "MIT",
  "author": "Matt Oakes <hello@mattoakes.net>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "pnpm run build --watch",
    "types": "tsc",
    "lint": "eslint",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.15.19",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "dependencies": {
    "node-hid": "^3.1.2"
  }
}
